generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id              String   @id @default(uuid())
  upc_id          String   
  title           String
  is_available    Boolean
  custom_provider String
  last_updated    DateTime @updatedAt
  product_type    String?
  external_id     String?
  price           Int
  currency        String

  @@unique([upc_id, custom_provider])
}